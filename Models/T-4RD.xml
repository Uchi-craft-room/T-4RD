<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>T-4RD.ac</path>


    <!-- Uncomment this section for FG 3.5 ALS Glass effects -->
    <effect>
        <name>T4glass</name>
        <inherits-from>Effects/glass</inherits-from>
        <object-name>canopy.glass.inn-RR</object-name>
    </effect>
    <effect>
        <name>T4windshield</name>
        <inherits-from>Effects/glass</inherits-from>
        <object-name>canopy.glass.inn-FR</object-name>
    </effect>

    <!-- Canopy -->
    <animation>
        <type>select</type>
        <condition>
            <not>
                <property>/sim/rendering/rembrandt/enabled</property>
            </not>
        </condition>
        <object-name>canopy.glass.inn-FR</object-name>
        <object-name>canopy.glass.inn-RR</object-name>
    </animation>

  <nasal>
    <load>
      var livery_update = aircraft.livery_update.new("Aircraft/T-4RD/Models/Liveries", 10);
    </load>
    <unload>
      livery_update.stop();
    </unload>
  </nasal>

  <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>glass_shader.png</texture>
    <object-name>canopy.glass.front</object-name>
    <object-name>canopy.glass.rear</object-name>
    <object-name>mirror.glass</object-name>
  </animation>


  <animation>
    <type>noshadow</type>
    <object-name>canopy.glass.front</object-name>
    <object-name>canopy.glass.rear</object-name>
    <object-name>mirror.glass</object-name>
  </animation>


  <animation>
    <type>material</type>
    <object-name>fuselage</object-name>
    <object-name>windshild_outer</object-name>
    <object-name>canopy_frame_out</object-name>
    <object-name>engine_left</object-name>
    <object-name>engine_right</object-name>
    <object-name>wing_upr_left</object-name>
    <object-name>wing_upr_right</object-name>
    <object-name>wing_lwr_left</object-name>
    <object-name>wing_lwr_right</object-name>
    <object-name>gear_hatch1_left</object-name>
    <object-name>gear_hatch2_left</object-name>
    <object-name>gear_hatch1_right</object-name>
    <object-name>gear_hatch2_right</object-name>
    <object-name>gear_nose_hatch1</object-name>
    <object-name>gear_nose_hatch2</object-name>
    <object-name>antenna</object-name>
    <object-name>aileron_right</object-name>
    <object-name>aileron_left</object-name>
    <object-name>flap_right</object-name>
    <object-name>flap_left</object-name>
    <object-name>flap_hinge0_rh_out</object-name>
    <object-name>flap_hinge0_rh_in</object-name>
    <object-name>flap_hinge1_rh_out</object-name>
    <object-name>flap_hinge1_rh_in</object-name>
    <object-name>flap_hinge0_lh_out</object-name>
    <object-name>flap_hinge0_lh_in</object-name>
    <object-name>flap_hinge1_lh_out</object-name>
    <object-name>flap_hinge1_lh_in</object-name>
    <object-name>vstab</object-name>
    <object-name>rudder</object-name>
    <object-name>stabilator</object-name>
    <object-name>vstabFin1</object-name>
    <object-name>vstabFin2</object-name>
    <object-name>antenna2</object-name>
    <texture-prop>texture</texture-prop>
    <texture>texture.png</texture>
    <property-base>sim/model/livery</property-base>
    <texture-prop>texture</texture-prop>
    <texture>Aircraft/T-4RD/Models/Liveries/Default.png</texture>
    <shininess>128</shininess>
    <specular>
      <red-prop>specular/red</red-prop>
      <green-prop>specular/green</green-prop>
      <blue-prop>specular/blue</blue-prop>
    </specular>

    <emission>
      <red-prop>emission/red</red-prop>
      <green-prop>emission/green</green-prop>
      <blue-prop>emission/blue</blue-prop>
    </emission>

  </animation>

<!-- Pushback -->
<model>
 <name>Pushback</name>
 <path>Aircraft/T-4RD/Models/towing_car/towing_car.xml</path>
  <condition>
 <less-than-equals>
     <property>velocities/airspeed-kt</property>
     <value>80</value>
 </less-than-equals>
  </condition>
   <offsets>
            <x-m>-4.2</x-m>
            <y-m>-0.00</y-m>
            <z-m>-1.45</z-m>
 </offsets>
</model> 

  <!-- Interior -->
  <model>
    <path>Aircraft/T-4RD/Models/Interior/interior.xml</path>
    <offsets>
      <x-m> 0 </x-m>
      <y-m> 0 </y-m>
      <z-m> 0 </z-m>
      <roll-deg>    0 </roll-deg>
      <pitch-deg>   0 </pitch-deg>
      <heading-deg> 0 </heading-deg>
    </offsets>
  </model>

  <!-- Shade -->
  <model>
    <name>RRSEATSHADE</name>
    <path>Aircraft/T-4RD/Models/shade/shade.ac</path>
    <offsets>
      <x-m> -1.6 </x-m>
      <y-m> 0 </y-m>
      <z-m> 0.7 </z-m>
      <roll-deg>    0 </roll-deg>
      <pitch-deg>   0 </pitch-deg>
      <heading-deg> 0 </heading-deg>
    </offsets>
  </model>

    <animation>
        <type>select</type>
        <object-name>RRSEATSHADE</object-name>
        <condition>
                <property>/controls/seat/shade</property>
        </condition>
    </animation>



  <!--  OSG -->
  <model>
    <name>shadowOSG</name>
    <path>Aircraft/T-4RD/Models/shadow.xml</path>
  </model>

 <animation>
    <type>select</type>
    <object-name>shadowOSG</object-name>
    <condition>
      <and>
      <equals>
        <property>/sim/rendering/rembrandt/enabled</property>
        <value>false</value>
      </equals>
      <equals>
        <property>/sim/replay/replay-state</property>
        <value>0</value>
      </equals>
      <property>gear/gear/ground-is-solid</property>
      </and>
    </condition>
  </animation>

  <!-- Exzaust Right-->
  <model>
    <path>Aircraft/T-4RD/Models/effects/smoke/smokeR.xml</path>
    <offsets>
      <x-m>  4.2 </x-m>
      <y-m>  0.6 </y-m>
      <z-m> -0.2 </z-m>
    </offsets>
  </model>

  <!-- Exzaust Left-->
  <model>
    <path>Aircraft/T-4RD/Models/effects/smoke/smokeL.xml</path>
    <offsets>
      <x-m>  4.2 </x-m>
      <y-m>  -0.6 </y-m>
      <z-m> -0.2 </z-m>
    </offsets>
  </model>

  <!-- TipTrail Right-->
  <model>
    <path>Aircraft/T-4RD/Models/effects/tiptrail/tiptrail.xml</path>
    <offsets>
      <x-m>  1.5</x-m>
      <y-m> -5.034 </y-m>
      <z-m>  -0.1</z-m>
    </offsets>
  </model>

  <!-- TipTrail left-->
  <model>
    <path>Aircraft/T-4RD/Models/effects/tiptrail/tiptrail.xml</path>
    <offsets>
      <x-m> 1.5 </x-m>
      <y-m> 5.034 </y-m>
      <z-m> -0.1 </z-m>
    </offsets>
  </model>

  <!-- stabilator -->
  <animation>
    <type>rotate</type>
    <object-name>stabilator</object-name>
    <property>surface-positions/elevator-pos-norm</property>
    <interpolation>
      <entry><ind> -0.75</ind><dep> -12.0 </dep></entry>
      <entry><ind>  0.00 </ind><dep>   0.0 </dep></entry>
      <entry><ind>  0.75 </ind><dep>   9.5 </dep></entry>
    </interpolation>
    <center>
      <x-m> 5.177</x-m>
      <y-m> 0.000 </y-m>
      <z-m> 0.450 </z-m>
    </center>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>

