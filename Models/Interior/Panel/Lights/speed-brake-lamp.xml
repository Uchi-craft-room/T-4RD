<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

 <path>redlight.ac</path>

  <animation>
    <type>select</type>
    <object-name>light</object-name>
    <condition>
     <and>
     <greater-than>
       <property>controls/flight/speedbrake</property>
       <value>0.1</value>
     </greater-than>
     <greater-than>
	   <property>systems/electrical/volts</property>
	   <value>24</value>
     </greater-than>
    </and>
    </condition>
  </animation>

</PropertyList>



