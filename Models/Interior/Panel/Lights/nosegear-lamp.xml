<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

 <path>greenlight.ac</path>

  <animation>
    <type>select</type>
    <object-name>light</object-name>
    <condition>
      <and>
        <equals>
            <property>gear/gear/position-norm</property>
            <value>1</value>
        </equals>
	<greater-than>
	   <property>systems/electrical/volts</property>
	   <value>24</value>
	</greater-than>
    </and>
    </condition>
  </animation>

</PropertyList>

