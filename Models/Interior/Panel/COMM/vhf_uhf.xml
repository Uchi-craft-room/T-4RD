            <repeatable>true</repeatable>
            <binding>
                <command>property-adjust</command>
                <property>instrumentation/VHF-22/comm1</property>
                <step>10000</step>
                <min>118000</min>
                <max>399990</max>
                <wrap>true</wrap>
            </binding>
        </action>
    </animation>

    <animation>
        <type>pick</type>
