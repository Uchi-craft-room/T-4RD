<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

    <path>rpm.ac</path>

    <animation>
        <type>material</type>
        <object-name>PropRPM.face</object-name>
        <object-name>Prpm.1</object-name>
        <object-name>Prop.ind</object-name>
        <object-name>PropRPMneedle</object-name>    
     <emission>
      <factor-prop>systems/electrical/outputs/instrument-lights-norm</factor-prop>
      <red>   0.5 </red>
      <green> 0.5 </green>
      <blue>  0.5 </blue>
    </emission>
  </animation>


    <animation>
        <type>rotate</type>
        <object-name>PropRPMneedle</object-name>
        <property>engines/engine[0]/n1</property>
        <interpolation>
            <entry><ind>0</ind><dep>0</dep></entry>
            <entry><ind>100</ind><dep>135</dep></entry>
        </interpolation>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>PropRPMneedle</object-name>
        <property>/controls/engines/engine/starter-position-norm</property>
        <interpolation>
            <entry><ind>0</ind><dep>0.0</dep></entry>
            <entry><ind>1</ind><dep>135</dep></entry>
        </interpolation>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

</PropertyList>
















