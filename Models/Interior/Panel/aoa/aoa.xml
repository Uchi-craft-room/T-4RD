<?xml version="1.0"?>


<PropertyList>

	<path>aoa.ac</path>

	<animation>
		<type>rotate</type>
		<object-name>needle</object-name>
		<property>/orientation/alpha-deg</property>
		<interpolation>
			<entry><ind>0</ind><dep>0</dep></entry>
			<entry><ind>10</ind><dep>90</dep></entry>
			<entry><ind>20</ind><dep>180</dep></entry>
			<entry><ind>30</ind><dep>270</dep></entry>
		</interpolation>
		<center>
			<x-m>0</x-m>
			<y-m>0</y-m>
			<z-m>0</z-m>
		</center> 
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>


	<animation>
		<object-name>mount</object-name>
                <object-name>face</object-name>
                <object-name>needle</object-name> 
		<type>material</type>
		<emission> 
			<factor-prop>systems/electrical/outputs/instrument-lights-norm</factor-prop>
			<red>0.5</red>
			<green>0.5</green>
			<blue>0.5</blue>
		</emission>
	</animation>


	<animation>
		<object-name>accel</object-name>
		<type>noshadow</type>
	</animation>


	<!-- *** LoD *** -->
	<animation>
		<type>range</type>
		<min-m>0</min-m>
		<max-m>5</max-m>
	</animation>

</PropertyList>





