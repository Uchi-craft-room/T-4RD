<?xml version="1.0"?>

<PropertyList>

 <name>ASI</name>
 <path>asi.ac</path>

 <animation>
  <type>rotate</type>
  <object-name>ASI_face</object-name>
  <object-name>ASI_mach-scale</object-name>
  <property>instrumentation/airspeed-indicator/indicated-speed-kt</property>
  <interpolation>
   <entry><ind>0</ind><dep>0</dep></entry>
   <entry><ind>50</ind><dep>10</dep></entry>
   <entry><ind>100</ind><dep>70</dep></entry>
   <entry><ind>120</ind><dep>90</dep></entry>
   <entry><ind>150</ind><dep>135</dep></entry>
   <entry><ind>200</ind><dep>178</dep></entry>
   <entry><ind>250</ind><dep>200</dep></entry>
   <entry><ind>300</ind><dep>235</dep></entry>
   <entry><ind>400</ind><dep>280</dep></entry>
   <entry><ind>500</ind><dep>300</dep></entry>
   <entry><ind>600</ind><dep>330</dep></entry>
   <entry><ind>700</ind><dep>350</dep></entry>
  </interpolation>
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>ASI_mach-scale</object-name>
  <property>/velocities/mach</property>
  <interpolation>
   <entry><ind>0</ind><dep>0</dep></entry>
   <entry><ind>0.8</ind><dep>90</dep></entry>
   <entry><ind>1.4</ind><dep>180</dep></entry>
   <entry><ind>2.2</ind><dep>270</dep></entry>
   <entry><ind>2.6</ind><dep>315</dep></entry>
  </interpolation>
  <axis>
   <x>1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>


 <!-- Lightning -->

 <animation>
  <object-name>ASI_mount</object-name>
  <object-name>ASI_face</object-name>
  <object-name>ASI_mach-scale</object-name>
  <object-name>ASI_bkg</object-name>
  <object-name>ASI_bkg2</object-name>
  <object-name>ASI_safe-speed-bug</object-name>
  <object-name>ASI_knob</object-name>
  <type>material</type>
  <emission>
   <factor-prop>systems/electrical/outputs/instrument-lights-norm</factor-prop>
   <red>0.6</red>
   <green>0.6</green>
   <blue>0.6</blue>
  </emission>
 </animation>


 <animation>
  <type>range</type>
  <min-m>0</min-m>
  <max-m>10</max-m>
 </animation>

</PropertyList>
