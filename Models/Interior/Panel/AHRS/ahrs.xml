<?xml version="1.0"?>

<PropertyList>
<path>ahrs.ac</path>

<!--   LATITUDE  -->
 <animation>
  <type>textranslate</type>
  <object-name>drum-1</object-name>
  <property>instrumentation/gps/indicated-latitude-deg</property>
  <factor>10</factor>
  <offset>0</offset>
  <step>1</step>
  <scroll>1</scroll>
  <center>
   <x-m>0</x-m>
   <y-m>0</y-m>
   <z-m>0</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>-1</y>
   <z>0</z>
  </axis>
 </animation>




