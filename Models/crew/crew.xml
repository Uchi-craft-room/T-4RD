<?xml version="1.0"?>

<PropertyList>

  <path>crew.ac</path>

  <animation>
    <!-- Objets opaques -->
    <object-name>arm1-RH</object-name>
    <object-name>arm2-RH</object-name>
    <object-name>fing1-RH</object-name>
    <object-name>fing2-RH</object-name>
    <object-name>fing4-RH</object-name>
    <object-name>fing5-RH</object-name>
    <object-name>arm1-LH</object-name>
    <object-name>arm2-LH</object-name>
    <object-name>fing1-LH</object-name>
    <object-name>fing2-LH</object-name>
    <object-name>fing4-LH</object-name>
    <object-name>fing5-LH</object-name>
    <!-- Objets transparents -->
  </animation>

  <animation>
    <name>ARM-RH</name>
    <object-name>arm2-RH</object-name>
    <object-name>fing1-RH</object-name>
    <object-name>fing2-RH</object-name>
    <object-name>fing4-RH</object-name>
    <object-name>fing5-RH</object-name>
  </animation>

  <animation>
    <name>ARM-LH</name>
    <object-name>arm2-LH</object-name>
    <object-name>fing1-LH</object-name>
    <object-name>fing2-LH</object-name>
    <object-name>fing4-LH</object-name>
    <object-name>fing5-LH</object-name>
  </animation>

<!--             arm RH               -->
  <animation>
    <type>select</type>
    <object-name>fing2-RH</object-name>
    <condition>
      <and>
       <equals>
           <property>controls/engines/engine[1]/starter</property>
           <value>false</value>
       </equals>
      <greater-than>
      <property>/controls/engines/engine/starter-position-norm</property>
      <value>0.4</value>
      </greater-than>
     </and>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>fing5-RH</object-name>
    <condition>
      <and>
       <equals>
           <property>controls/engines/engine[1]/starter</property>
           <value>false</value>
       </equals>
      <greater-than>
      <property>/controls/engines/engine/starter-position-norm</property>
      <value>0.6</value>
      </greater-than>
     </and>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>fing4-RH</object-name>
    <condition>
      <and>
       <equals>
           <property>controls/engines/engine[1]/starter</property>
           <value>false</value>
       </equals>
       <greater-than>
          <property>/controls/engines/engine/starter-position-norm</property>
          <value>0.8</value>
       </greater-than>
      </and>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>fing1-RH</object-name>
    <condition>
      <and>
        <equals>
           <property>controls/engines/engine[1]/starter</property>
           <value>false</value>
        </equals>
        <equals>
         <property>/controls/engines/engine/starter-position-norm</property>
         <value>1.0</value>
        </equals>
     </and>
    </condition>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>ARM-RH</object-name>
    <property>controls/engines/engine/starter</property>
    <factor> 300 </factor>
    <center>
      <x-m> -9.525 </x-m>
      <y-m> -0.404 </y-m>
      <z-m> -0.228 </z-m>
    </center>
    <axis>
      <x> 1.000 </x>
      <y> 0.000 </y>
      <z> 0.000 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>ARM-RH</object-name>
    <property>controls/engines/engine[1]/starter</property>
    <factor> 60 </factor>
    <center>
      <x-m> -9.525 </x-m>
      <y-m> -0.404 </y-m>
      <z-m> -0.228 </z-m>
    </center>
    <axis>
      <x> 1.000 </x>
      <y> 0.000 </y>
      <z> 0.000 </z>
    </axis>
  </animation>

<!--             arm LH               -->
  <animation>
    <type>select</type>
    <object-name>fing2-LH</object-name>
    <condition>
      <and>
       <equals>
           <property>controls/engines/engine[1]/starter</property>
           <value>true</value>
       </equals>
      <greater-than>
      <property>/controls/engines/engine[1]/starter-position-norm</property>
      <value>0.4</value>
      </greater-than>
     </and>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>fing5-LH</object-name>
    <condition>
      <and>
       <equals>
           <property>controls/engines/engine[1]/starter</property>
           <value>true</value>
       </equals>
      <greater-than>
      <property>/controls/engines/engine[1]/starter-position-norm</property>
      <value>0.6</value>
      </greater-than>
     </and>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>fing4-LH</object-name>
    <condition>
      <and>
       <equals>
           <property>controls/engines/engine[1]/starter</property>
           <value>true</value>
       </equals>
       <greater-than>
          <property>/controls/engines/engine[1]/starter-position-norm</property>
          <value>0.8</value>
       </greater-than>
      </and>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>fing1-LH</object-name>
    <condition>
      <and>
        <equals>
           <property>controls/engines/engine[1]/starter</property>
           <value>true</value>
        </equals>
        <equals>
         <property>/controls/engines/engine[1]/starter-position-norm</property>
         <value>1.0</value>
        </equals>
     </and>
    </condition>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>ARM-LH</object-name>
    <property>controls/engines/engine[1]/starter</property>
    <factor> 60 </factor>
    <center>
      <x-m> -9.525 </x-m>
      <y-m>  0.392 </y-m>
      <z-m> -0.225 </z-m>
    </center>
    <axis>
      <x> 1.000 </x>
      <y> 0.000 </y>
      <z> 0.000 </z>
    </axis>
  </animation>

</PropertyList>



